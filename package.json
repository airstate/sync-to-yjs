{
  "name": "sync-to-yjs",
  "version": "1.0.2",
  "description": "A simple utility that syncs / converts JS objects and arrays into `Y.Map` and `Y.Array` respectively",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "test": "jest"
  },
  "devDependencies": {
    "typescript": "5.4.3",
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "ts-jest": "29.1.2",
    "prettier": "3.2.5",
    "microdiff": "1.5.0"
  },
  "peerDependencies": {
    "yjs": ">=13"
  },
  "keywords": [
    "yjs", "sync", "convert"
  ],
  "contributors": [
    {
      "name": "Omran Jamal",
      "email": "omran@omranjamal.me",
      "url": "https://omranjamal.me"
    },
    {
      "name": "AirState",
      "email": "hello@airstate.dev",
      "url": "https://airstate.dev"
    }
  ],
  "license": "MIT",
  "include": [
    "dist/**/*"
  ]
}
